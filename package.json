{
  "main": "./src/index.js",
  "name": "structures",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "test": "ava --verbose",
    "format": "prettier-standard '{src,test,benchmark}/**/*.js'",
    "docs": "rm -rf docs && jsdoc -c config/jsdoc.json && git add docs",
    "benchmark": "node ./benchmark"
  },
  "keywords": [
    "data",
    "structures"
  ],
  "author": "Justin Fuller <justindanielfuller@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "ava": "^2.3.0",
    "benchmark": "^2.1.4",
    "chalk": "^2.4.2",
    "husky": "^3.0.5",
    "jsdoc": "^3.6.1",
    "lint-staged": "^9.2.5",
    "prettier-standard": "^14.0.3"
  },
  "lint-staged": {
    "linters": {
      "{src,test,benchmark}/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test && npm run docs"
    }
  },
  "dependencies": {
    "docdash": "^1.1.0"
  }
}
